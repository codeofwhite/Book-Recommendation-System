{
  "name": "clickhouse-user-behavior-logs-sink",
  "config": {
    "connector.class": "com.clickhouse.kafka.connect.ClickHouseSinkConnector",
    "tasks.max": "1",
    "consumer.override.max.poll.records": "5000",
    "consumer.override.max.partition.fetch.bytes": "5242880",
    "hostname": "clickhouse",
    "port": "8123",
    "ssl": "false",
    "jdbcConnectionProperties": "",
    "database": "default",
    "username": "default",
    "password": "",
    "topics": "user_behavior_logs",
    "table.name": "user_behavior_logs:user_behavior_logs_raw",
    "auto.create.tables": "true",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "insert.mode": "insert",
    "batch.size": "1000",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "errors.tolerance": "all",
    "errors.deadletterqueue.topic.name": "user_behavior_logs_dlq",
    "errors.deadletterqueue.topic.replication.factor": "1",
    "transforms": "ExtractFields",
    "transforms.ExtractFields.type": "org.apache.kafka.connect.transforms.ExtractField$Value",
    "transforms.ExtractFields.field": "payload",
    "errors.retry.timeout": "60",
    "exactlyOnce": "false",
    "clickhouseSettings": "",
    "connection.timeout.ms": "60000",
    "connection.attempts": "10",
    "connection.backoff.ms": "5000"
  }
}